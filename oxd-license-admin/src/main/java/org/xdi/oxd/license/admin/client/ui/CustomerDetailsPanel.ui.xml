<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:cellview="urn:import:com.google.gwt.user.cellview.client"
        >

    <g:VerticalPanel ui:field="rootPanel">
        <g:HTML>
            <h3>Name:</h3>
        </g:HTML>
        <g:HTML ui:field="nameField"/>
        <g:HTML>
            <h3>License Id:</h3>
        </g:HTML>
        <g:HorizontalPanel>
            <g:Button ui:field="addLicenseId" enabled="false">Add License Id</g:Button>
            <g:Button ui:field="removeLicenseId" enabled="false">Remove License Id</g:Button>
        </g:HorizontalPanel>
        <cellview:CellTable ui:field="licenseIdTable" width="100%"/>
    </g:VerticalPanel>
</ui:UiBinder>
